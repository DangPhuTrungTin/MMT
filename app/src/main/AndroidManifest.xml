<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dangphutrungtin.clientapp">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainClient">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WaitRoomActivity"></activity>
        <activity android:name=".User1"></activity>
        <activity android:name=".User2"></activity>
        <activity android:name=".Anonymous"></activity>
        <activity android:name=".InGameActivity"></activity>
        <activity android:name=".ResultActivity"></activity>
        <activity android:name=".FreeSetForUnKnownPerson"></activity>
        <receiver android:name=".NotificationReceiver"/>
    </application>

</manifest>